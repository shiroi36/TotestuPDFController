                  <sign>f00</sign>
                  <batfile>C:\Users\user\Documents\NetBeansProjects\TotestuPDFController\files\totetuPDFcontroller_start.bat</batfile>
            </engine>

            <engine>
                  <eid>"stkr-read"</eid>
                  <sign>f01</sign>
                  <batfile>C:\Users\user\Documents\NetBeansProjects\TotestuPDFController\files\totetuPDFcontroller_start.bat</batfile>
            </engine>

            <engine>
                  <eid>"ripc-write"</eid>
                  <sign>f02</sign>
                  <batfile>C:\Users\user\Documents\NetBeansProjects\TotestuPDFController\files\TtotetuPDFcontroller_start.bat</batfile>
            </engine>

            <engine>
                  <eid>"ripc-read"</eid>
                  <sign>f03</sign>
                  <batfile>C:\Users\user\Documents\NetBeansProjects\TotestuPDFController\files\totetuPDFcontroller_start.bat</batfile>
            </engine>

            <engine>
                  <eid>"read-all"</eid>
                  <sign>f04</sign>
                  <batfile>C:\Users\user\Documents\NetBeansProjects\TotestuPDFController\files\totetuPDFcontroller_start.bat</batfile>
            </engine>

